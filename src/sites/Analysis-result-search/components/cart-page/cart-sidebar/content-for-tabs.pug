mixin Content-for-tabs(param={})
            .cart-order__block.cart-order__place
                if(!param.notAuxTitle)
                    p.n-title-sm Где буду сдавать:
                else
                    .n-title-aux=param.title
                if(!param.noCheck)
                    .cart-order__radio
                        +Btn-radio({
                            radioId: 'place-1',
                            name: 'place',
                            label: 'В медицинском центре',
                            labelClass: 'n-title-xs',
                            checked: true
                        })
                        +Btn-radio({
                            radioId: 'place-2',
                            name: 'place',
                            label: 'На дому',
                            labelClass: 'n-title-xs'
                        })
                        if(param.data)
                            .cart-order__data
                                .cart-order__input-wrapper
                                    input.form__input(type="text" name="address" id="address" placeholder=" " required)
                                    label.form__label(for="address") Введите ваш адрес*
                                    svg(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20.000000" height="20.734375" viewBox="0 0 20 20.7344" fill="none")
